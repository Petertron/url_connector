<?xml version="1.0" encoding="UTF-8"?>
<extension id="url_connector" status="released" xmlns="http://symphony-cms.com/schemas/extension/1.0">
    <name>URL Connector</name>
    <description>Allows URLs to be mapped to pages with optional parameter testing. Also performs URL redirection.</description>
    <repo type="github">https://github.com/Petertron/url_connector</repo>
    <url type="discuss">http://www.getsymphony.com/discuss/thread/110707/</url>
    <types>
        <type>Routing</type>
    </types>
    <authors>
        <author>
            <name github="Petertron" symphony="Petertron">Peter Skirenko</name>
            <website>http://www.getsymphony.com/get-involved/member/Petertron/</website>
        </author>
    </authors>
    <releases>
        <release version="0.1.1" date="2015-07-29" min="2.5" max="3.0">
            - Routing bug fixed.
            - Modified for to be compatible with Symphony 3.0.0-alpha.1.
        </release>
        <release version="0.1.0" date="2015-07-03" min="2.5" max="2.6.2">
            - First release.
        </release>
    </releases>
</extension>
